def calculate_gpa(grades, total_subjects=5):
    total = sum(grades)
    gpa = total / total_subjects
    return round(gpa, 2)

grades = [3.2, 2.8, 3.5, 2.9, 3.1]
result = calculate_gpa(grades=grades, total_subjects=5)

print(f"GPA Result: {result}")
