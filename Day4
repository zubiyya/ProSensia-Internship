student_ids = (101, 102, 103, 104)

courses = {"Python", "AI", "ML"}

courses.add("Data Science")
courses.add("Python")
courses.remove("ML")

print("Student IDs (Immutable):", student_ids)
print("Available Courses:", courses)

print("Course List:")
for course in courses:
    print("-", course)
